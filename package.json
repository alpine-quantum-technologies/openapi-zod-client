    },
    "devDependencies": {
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.27.11",
        "@preconstruct/cli": "^2.8.10",
        "conventional-changelog-cli": "^2.2.2",
        "prettier": "^2.8.8",
        "tsx": "^3.14.0",
        "typescript": "^4.9.5"
    }
}
